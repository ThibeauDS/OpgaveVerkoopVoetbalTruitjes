<Page x:Class="VerkoopVoetbalTruitjes.WPF.Pages.DashbordPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:VerkoopVoetbalTruitjes.WPF.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DashbordPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBlock Text="Dashboard" FontSize="15"/>

        <StackPanel Grid.Row="1">
            <TextBlock Name="MessageTxt" Text="" Loaded="MessageTxt_Loaded"/>
        </StackPanel>
    </Grid>
</Page>
